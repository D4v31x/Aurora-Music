<?xml version="1.0" encoding="utf-8"?>
<!-- Preview shown in widget picker -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="28dp" />
            <gradient
                android:type="linear"
                android:startColor="#CC1A0B2E"
                android:centerColor="#B32D1B4E"
                android:endColor="#CC16213E"
                android:angle="135" />
            <stroke
                android:width="1dp"
                android:color="#33FFFFFF" />
        </shape>
    </item>
</layer-list>
